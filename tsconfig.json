{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "esModuleInterop": true,
    "declaration": true,
    "declarationMap": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "noEmit": true,
    "jsx": "react",
    "removeComments": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "allowJs": true, 
    "moduleResolution": "Node"
  
  }
}
